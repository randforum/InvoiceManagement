@{
    @inject IGeneralService _generalService;
    var GeneralData = _generalService.GetGeneralSettings();
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Admin Panel</title>
    <!-- Custom fonts for this template-->
    <link href="~/design/webadmin/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <!-- Admin CSS -->
    <link href="~/design/webadmin/css/sb-admin-2.css" rel="stylesheet" />
    <!-- Custom CSS -->
    <link href="~/design/webadmin/css/custom.css" rel="stylesheet" />
    <!-- Core plugin JavaScript-->
    <script src="~/design/webadmin/vendor/jquery/jquery.min.js"></script>
    <script src="~/design/webadmin/vendor/jquery-easing/jquery.easing.min.js"></script>
    <!-- Text Editor Files-->
    <link href="~/design/webadmin/vendor/summernote-0.8.12-dist/dist/summernote-bs4.css" rel="stylesheet" />
    <script src="~/design/webadmin/vendor/summernote-0.8.12-dist/dist/summernote-bs4.min.js"></script>
    <!-- Bootstrap core JavaScript-->
    <script src="~/design/webadmin/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- Jquery UI Files -->
    <link href="~/design/webadmin/vendor/jquery/ui/jquery-ui.css" rel="stylesheet" />
    <script src="~/design/webadmin/vendor/jquery/ui/jquery-ui.js"></script>
    <!-- Custom CSS -->
    <link href="~/design/custom/css/custom.css" rel="stylesheet" />
    <link href="~/design/custom/css/pager.css" rel="stylesheet" />
    <!--  Scripts For Client Side Validation -->
    <script src="~/design/client-side-validation/jquery.validate.min.js"></script>
    <script src="~/design/client-side-validation/jquery.validate.unobtrusive.min.js"></script>


</head>
<body id="page-top">
    <!-- Page Wrapper -->
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">
            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" asp-controller="WebMaster" asp-action="Dashboard">
                <div class="sidebar-brand-icon rotate-n-15">
                    <i class="fas fa-laugh-wink"></i>
                </div>
                <div class="sidebar-brand-text mx-3">AIB-Admin</div>
            </a>
            <!-- Divider -->
            <hr class="sidebar-divider my-0" />
            <!-- Nav Item - Dashboard -->
            <li class="nav-item">
                <a class="nav-link" asp-controller="WebMaster" asp-action="Dashboard">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span>
                </a>
            </li>
            <!-- Divider -->
            <hr class="sidebar-divider" />
            <!-- Heading -->
            <div class="sidebar-heading">
                Useful Links
            </div>
            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="SelectLayout" asp-controller="IB">
                    <i class="fas fa-fw fa-file-alt"></i>
                    <span>Create Invoice</span>
                </a>

            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="InvoiceHistoryList" asp-controller="IB">
                    <i class="fas fa-fw fa-file"></i>
                    <span>Invoice History</span>
                </a>

            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="CurrencyMaster" asp-controller="Webmaster">
                    <i class="fas fa-fw fa-money-bill"></i>
                    <span>Currency Management</span>
                </a>

            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="CustomerMaster" asp-controller="Customer">
                    <i class="fas fa-fw fa-address-book"></i>
                    <span>Cutomer Management</span>
                </a>

            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="ProductMaster" asp-controller="Product">
                    <i class="fas fa-fw fa-list"></i>
                    <span>Product Management</span>
                </a>

            </li>

            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="GeneralSettings" asp-controller="WebMaster">
                    <i class="fas fa-fw fa-cog"></i>
                    <span>General Settings</span>
                </a>

            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="EmailSettingUpdate" asp-controller="WebMaster">
                    <i class="fas fa-fw fa-trash"></i>
                    <span>Email Settings</span>
                </a>

            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="ProfileSettings" asp-controller="WebMaster">
                    <i class="fas fa-fw fa-user"></i>
                    <span>Profile Settings</span>
                </a>

            </li>
        </ul>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">
            <!-- Main Content -->
            <div id="content">
                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
                    <!-- Sidebar Toggle (Topbar) -->
                    <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
                        <i class="fa fa-bars"></i>
                    </button>
                    <!-- Topbar Navbar -->
                    <ul class="navbar-nav ml-auto">
                        <!-- Nav Item - Search Dropdown (Visible Only XS) -->
                        <li class="nav-item dropdown no-arrow d-sm-none">
                            <a class="nav-link dropdown-toggle" href="#" id="searchDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fas fa-search fa-fw"></i>
                            </a>
                        </li>

                        <!-- Nav Item - User Information -->
                        <li class="nav-item dropdown no-arrow">
                            <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <span class="mr-2 d-none d-lg-inline text-gray-600 small">AIB-Admin</span>
                                <img class="img-profile rounded-circle" src="~/design/webadmin/admin.jpg">
                            </a>
                            <!-- Dropdown - User Information -->
                            <div class="dropdown-menu dropdown-menu-right shadow animated--grow-in" aria-labelledby="userDropdown">

                                <a class="dropdown-item" asp-action="Logout" asp-controller="WebMaster">
                                    <i class="fas fa-sign-out-alt fa-sm fa-fw mr-2 text-gray-400"></i>
                                    Logout
                                </a>
                            </div>
                        </li>
                    </ul>
                </nav>
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <h1 class="h3 mb-4 text-gray-800" style="border-bottom:1px solid #dad7d7;padding-bottom:10px;">@ViewBag.PageHeader</h1>
                            @RenderBody()

                        </div>
                    </div>
                </div>
                <!-- /.container-fluid -->
            </div>
            <!-- End of Main Content -->
            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy;  @System.DateTime.UtcNow.Year  </span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->
        </div>
        <!-- End of Content Wrapper -->
    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Custom scripts for all pages-->
    <script src="~/design/webadmin/js/sb-admin-2.js"></script>
    <script src="~/design/custom/js/custom.js"></script>
</body>
</html>